const menu = {_meal:'',_price:0 ,

set meal(mealToCheck) { if (typeof mealToCheck === 'string' ) {
   return this._meal = mealToCheck
} },
set price(priceToCheck) { if ( typeof priceToCheck === 'number'){
  return this._price = priceToCheck
}
},


get todaysSpecial (){ if(this._meal && this._price){
  return `this meal is  ${this._meal}  and the price is  ${this._price}`
}
else {
  return "nonce meal not set correctly"
}

}

} 




 menu.meal = "pizza"
 menu.price = 8
console.log(menu)
console.log(menu.todaysSpecial)
